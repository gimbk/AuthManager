	</parent>
	<groupId>com.microservice</groupId>
	<artifactId>authManager</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>authManager</name>
	<description>Demo project for Spring Boot</description>
	<url/>
	<licenses>
		<license/>
	</licenses>
	<developers>
		<developer/>
	</developers>
	<scm>
		<connection/>
		<developerConnection/>
		<tag/>
		<url/>
